{
  "name": "bun",
  "type": "module",
  "version": "0.1.0",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.1.0",
    "pre-commit": "^1.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/jwt": "^0.8.0",
    "@elysiajs/swagger": "^0.8.5",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "elysia": "^0.8.17",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.4",
    "zod": "^3.23.6"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "swc src -d dist --source-maps",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "update-version": "git add . && git push"
  },
  "pre-commit": [
    "build",
    "update-version"
  ]
}